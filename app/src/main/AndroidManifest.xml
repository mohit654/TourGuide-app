<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MumbaiActivity" android:label="@string/place1" />
        <activity android:name=".GoaActivity" android:label="@string/place2"/>
        <activity android:name=".DelhiActivity" android:label="@string/place3"/>
        <activity android:name=".KolkataActivity" android:label="@string/place4"></activity>
    </application>

</manifest>